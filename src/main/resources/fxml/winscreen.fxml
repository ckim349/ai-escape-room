<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="room" focusTraversable="true" prefHeight="650.0" prefWidth="1000.0" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.WinScreenController">
   <children>
      <ImageView fitHeight="677.0" fitWidth="1167.0" layoutX="-144.0" layoutY="-5.0" preserveRatio="true">
         <image>
            <Image url="@../images/winscreen.png" />
         </image>
      </ImageView>
      <Label layoutX="281.0" layoutY="87.0" prefHeight="180.0" prefWidth="439.0" text="Congratulations! You win!" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="60.0" />
         </font>
      </Label>
      <Button fx:id="exitButton" layoutX="401.0" layoutY="471.0" mnemonicParsing="false" onAction="#onExitButtonClicked" prefHeight="90.0" prefWidth="200.0" text="Exit ">
         <font>
            <Font size="38.0" />
         </font></Button>
      <Button fx:id="restartButton" layoutX="400.0" layoutY="370.0" mnemonicParsing="false" onAction="#onClickRestartButton" prefHeight="90.0" prefWidth="200.0" text="Restart">
         <font>
            <Font size="38.0" />
         </font>
      </Button>
      <Label fx:id="winTimeLabel" alignment="CENTER" layoutX="295.0" layoutY="267.0" prefHeight="90.0" prefWidth="411.0" text="Time: 2:06.42" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="50.0" />
         </font>
      </Label>
   </children>
</Pane>
