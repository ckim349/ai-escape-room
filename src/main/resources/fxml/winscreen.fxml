<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="room" focusTraversable="true" prefHeight="720.0" prefWidth="1280.0" stylesheets="@../css/styles.css" xmlns="http://javafx.com/javafx/" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.WinScreenController">
   <children>
      <ImageView fitHeight="837.0" fitWidth="1424.0" layoutX="-144.0" layoutY="-5.0">
         <image>
            <Image url="@../images/winscreen.png" />
         </image>
      </ImageView>
      <Label layoutX="360.0" layoutY="68.0" prefHeight="180.0" prefWidth="593.0" text="Congratulations! You win!" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="60.0" />
         </font>
      </Label>
      <Button fx:id="exitButton" layoutX="549.0" layoutY="568.0" mnemonicParsing="false" onAction="#onExitButtonClicked" prefHeight="90.0" prefWidth="214.0" text="Exit ">
         <font>
            <Font size="38.0" />
         </font></Button>
      <Button fx:id="restartButton" layoutX="548.0" layoutY="467.0" mnemonicParsing="false" onAction="#onClickRestartButton" prefHeight="90.0" prefWidth="214.0" text="Restart">
         <font>
            <Font size="38.0" />
         </font>
      </Button>
      <Label fx:id="winTimeLabel" alignment="CENTER" layoutX="450.0" layoutY="381.0" prefHeight="90.0" prefWidth="411.0" text="Time: 2:06.42" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="System Bold" size="50.0" />
         </font>
      </Label>
   </children>
</Pane>
